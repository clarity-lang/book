    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@stacks/transactions": "^6.15.0",
    "chokidar": "^3.5.2",
    "http-server": "^14.1.1",
    "marked": "^2.1.3",
    "misbehave": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.24.0",
    "rollup": "^4.17.2",
    "serve": "^14.2.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vitest-environment-clarinet": "^2.0.0"
  }
